generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id*     hash string
  title*     String
  date*      premiere
  is running boolean
  language*  String

  main genre* String

  poster url String
}
